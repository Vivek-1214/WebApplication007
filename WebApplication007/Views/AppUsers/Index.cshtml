@Model List<AppUser>

	<html>
		<head>
			<title>Index View</title>
		<style>
			body {
				width:100%;
				background-image: url("https://media.istockphoto.com/id/844226534/photo/leaf-background.jpg?s=612x612&w=0&k=20&c=N4NPPNXFU5hPcThEbQ-wr4y64pqSKm-x5AMDZ0sPL5w=");
				background-size:cover;
				
			}
			
			h1{
				text-align: center;
    color: aliceblue;
    font-size: xx-large;
			}

			table{
		              background-color: whitesmoke;
    height: 16rem;
    margin: 1rem 18rem;
	height:5rem;
				  }
			table,tr,td,th{
				border:2px solid white;
			}

			input {
				height: 1rem;
				width: 1rem;
				margin: 1rem;
			}

			button {
				background-color: aqua;
            margin: 1rem 31rem;
                 width: 5rem;
                    height: 2rem;
			}
			button a{
				color:white;
				text-decoration: none;
			}

		</style>
		</head>
		<body>
			
			
			
			<table>
				<thead>
				<h1>AppUsers List</h1>
					<tr>
						<th>Id</th>
						<th>Email</th>
					<th>FirstName</th>
					<th> LastName</th>
					<th>MobileNo</th>
					<th>Gender</th>
					</tr>
				</thead>
				<tbody>

				
				@foreach (var AppUser in Model)
				{
					<tr>
						<td>@AppUser.Id</td>
						<td>@AppUser.Email</td>
						<td>@AppUser.FirstName</td>
						<td>@AppUser.LastName</td>
						<td>@AppUser.MobileNo</td>
						<td>@AppUser.Gender</td>

						<td><a href="AppUsers/Update/@AppUser.Id">Update</a></td>
						<td><a href="AppUsers/Delete/@AppUser.Id">Delete</a></td>
					</tr>
				}
				
			</tbody>
			</table>
		
		<button> <a href="AppUsers/Create/">Create</a> </button>

		</body>
		<footer>

		</footer>
	</html>